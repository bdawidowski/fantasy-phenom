<div class="row">
    <h1 align="center">New Googlesheet</h1>
    <div class="col-sm-6 col-sm-offset-3">
        <div class="panel panel-default">
            <div class="panel-body panel-padded">
                <div class="col-xs-10 col-xs-offset-1">
                  <%= form_for(@googlesheet) do |f| %>
                    <div class="form-group">
                      <%= f.hidden_field :sync, value: true %>
                    </div>
                    <div class="form-group">
                      <%= f.submit "SYNC DATA", class: "btn btn-primary btn-block" %>
                    </div>
                  <% end %>
                </div>
            </div>
        </div>
    </div>
</div>
