<% team_batting_r = game.team_batting_r %>
<% team_batting_l = game.team_batting_l %>
<% team_batting_h = game.team_batting_h %>
<% team_batting_a = game.team_batting_a %>
<div class="col-sm-12">
  <table class="table table-data text-center">
    <tbody>
    <tr>
      <td colspan="2"></td>
      <td>OBP</td>
      <td>wOBA</td>
      <td>ISO</td>
      <td>HC%</td>
      <td>FB%</td>
      <td>K%</td>
      <td></td>
      <td>OBP</td>
      <td>wOBA</td>
      <td>ISO</td>
      <td>HC%</td>
      <td>FB%</td>
      <td>K%</td>
      <td colspan="2"></td>
    </tr>
    <tr>
      <td colspan="2">
        TEAM BATTING v. RIGHT
      </td>
      <% team_batting_r[0..5].each do |tbr| %>
          <td><%= tbr %></td>
      <% end %>
      <td></td>
      <% team_batting_r[6..11].each do |tbr| %>
          <td><%= tbr %></td>
      <% end %>
      <td colspan="2">
        TEAM BATTING v. RIGHT
      </td>
    </tr>
    <tr>
      <td colspan="2">
        TEAM BATTING v. LEFT
      </td>
      <% team_batting_l[0..5].each do |tbl| %>
          <td><%= tbl %></td>
      <% end %>
      <td></td>
      <% team_batting_l[6..11].each do |tbl| %>
          <td><%= tbl %></td>
      <% end %>
      <td colspan="2">
        TEAM BATTING v. LEFT
      </td>
    </tr>
    <tr>
      <td colspan="2">
        TEAM BATTING HOME
      </td>
      <% team_batting_h[0..5].each do |tbh| %>
          <td><%= tbh %></td>
      <% end %>
      <td></td>

      <% team_batting_h[6..11].each do |tbh| %>
          <td><%= tbh %></td>
      <% end %>
      <td colspan="2">
        TEAM BATTING HOME
      </td>
    </tr>
    <tr>
      <td colspan="2">
        TEAM BATTING AWAY
      </td>
      <% team_batting_a[0..5].each do |tba| %>
          <td><%= tba %></td>
      <% end %>
      <td></td>

      <% team_batting_a[6..11].each do |tba| %>
          <td><%= tba %></td>
      <% end %>
      <td colspan="2">
        TEAM BATTING AWAY
      </td>
    </tr>
    </tbody>
  </table>
</div>