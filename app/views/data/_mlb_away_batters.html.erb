<% away_batters = game.away_batters %>

<div class="col-sm-6 col-sm-offset-3">
  <table class="table table-data text-center">
    <tbody>
      <tr>
        <td colspan="2"><%= game.away_team.upcase %></td>
        <td colspan="2" class="data-confirmed data-table-dark">CONFIRMED LINEUP</td>
      </tr>
    </tbody>
  </table>
</div>
<div class="col-sm-12">
  <table class="table table-data text-center">
    <tbody>
    <tr class="table-data-head">
      <td>POS</td>
      <td>FD</td>
      <td>POS</td>
      <td>DK</td>
      <td>NAME</td>
      <td>BATS</td>
      <td>PA</td>
      <td>OBP</td>
      <td>wOBA</td>
      <td>ISO</td>
      <td>HARD%</td>
      <td>FB%</td>
      <td>K%</td>
      <td>HR%</td>
      <td>StCast%</td>
      <td>MwOBA</td>
    </tr>
    <% away_batters.each_with_index do |batter, bi| %>
      <tr>
        <% if current_user_subscribed? %>
          <% batter.each_with_index do |b, i| %>
            <td class="batter-data-<%= i %> <%= "dollar-data" if i == 1 or i == 3%>"><%= b %></td>
          <% end %>
        <% else %>
          <% batter.each_with_index do |b, i| %>
              <td class="batter-data-<%= i %> <%= "dollar-data" if i == 1 or i == 3%>" id="<%= "data-blur" if i >= 13%>"><%= b %></td>
          <% end %>
        <% end %>
      </tr>
    <% end %>
    </tbody>
  </table>
</div>
