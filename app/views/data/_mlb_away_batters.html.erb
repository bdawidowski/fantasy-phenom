<% away_batters = game.away_batters %>
<div class="col-sm-6">
  <table class="table table-data text-center">
    <tbody>
      <tr>
        <td colspan="2"><%= game.away_team %></td>
        <td colspan="2">Confirmed</td>
      </tr>
    </tbody>
  </table>
</div>
<div class="col-sm-6">
  <table class="table table-data text-center">
    <tbody>
    <tr>
      <td>POS</td>
      <td>FD</td>
      <td>POS</td>
      <td>DK</td>
      <td>NAME</td>
      <td>BATS</td>
      <td>PA</td>
      <td>OBP</td>
      <td>wOBA</td>
      <td>HARD%</td>
      <td>FB%</td>
      <td>K%</td>
      <td>HR%</td>
      <td>StCast%</td>
      <td>MwOBA</td>
    </tr>
    <% away_batters.each do |batter| %>
      <tr>
        <% batter.each do |b| %>
          <td><%= b %></td>
        <% end %>
      </tr>
    <% end %>
    </tbody>
  </table>
</div>